---
layout: null
permalink: podcast/mencari-sakinah/feed/
---
<?xml version="1.0" encoding="UTF-8"?><rss version="2.0"
xmlns:content="http://purl.org/rss/1.0/modules/content/"
xmlns:wfw="http://wellformedweb.org/CommentAPI/"
xmlns:dc="http://purl.org/dc/elements/1.1/"
xmlns:atom="http://www.w3.org/2005/Atom"
xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
xmlns:rawvoice="http://www.rawvoice.com/rawvoiceRssModule/"
>

    <channel>
        <title>{{ site.name }}</title>
        <atom:link href="{{ site.url }}/feed/" rel="self" type="application/rss+xml" />
        <link>{{ site.url }}</link>
        <description>{{ site.description }}</description>
        <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>
        <language>en-US</language>
        <sy:updatePeriod>hourly</sy:updatePeriod>
        <sy:updateFrequency>1</sy:updateFrequency>
        <generator>http://jekyllrb.com/</generator>
        {% for post in site.posts limit: 15 %}
        <item>
            <title><![CDATA[{{ post.title | cdata_escape }}]]></title>
            <link>{{ site.url }}{{ post.url }}</link>
            <pubDate>{{ post.date | date: "%a, %d %b %Y %T %z" }}</pubDate>
            <dc:creator><![CDATA[{{ site.author | cdata_escape }}]]></dc:creator>
            {% for category in post.categories %}
            <category><![CDATA[{{ category | cdata_escape }}]]></category>
            {% endfor %}
            {% for category in post.tags %}
            <category><![CDATA[{{ category | cdata_escape }}]]></category>
            {% endfor %}
            <guid isPermaLink="true">{% if post.pre_https %}http://joebuhlig.com{% else %}{{ site.url }}{% endif %}{{ post.url }}</guid>
            <description>
                <![CDATA[{{ post.excerpt | strip_html | truncatewords: 50 | expand_urls: site.url | cdata_escape }}]]>
            </description>
            <content:encoded>
                <![CDATA[<img class="featured-image" src="{{ post.image | prepend: site.url }}" />{{ post.content | expand_urls: site.url | cdata_escape }}]]>
            </content:encoded>
        </item>
        {% endfor %}

    </channel>

</rss>